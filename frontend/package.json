{
  "name": "munch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "elm-format src --validate",
    "clean": "rm -rf ../nginx/dist/*",
    "start": "webpack-dev-server --hot --port 3000 --colors",
    "copy": "mkdir -p ../nginx/dist && cp assets/background.jpg ../nginx/dist/background.jpg",
    "build:app": "NODE_ENV=prod FILE='app' webpack --mode=production",
    "build:sw": "NODE_ENV=prod FILE='sw' webpack --mode=production",
    "build": "export HASH=$(head /dev/urandom | tr -dc A-Za-z0-9 | head -c 13) && yarn copy && yarn build:sw && yarn build:app"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/przemyslawjanpietrzak/munch.git"
  },
  "author": "Przemyslaw Jan Pietrzak",
  "license": "MIT",
  "homepage": "https://bitbucket.org/przemyslawjanpietrzak/munch#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "elm": "^0.18.0",
    "elm-format": "^0.6.1-alpha",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^4.5.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "idb": "^2.1.3",
    "material-design-lite": "^1.3.0",
    "materialize": "^1.0.0",
    "materialize-css": "^1.0.0-rc.2"
  }
}
